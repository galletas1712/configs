{
    "languageserver": {
        "rust": {
            "command": "ra_lsp_server",
            "filetypes": ["rust"],
            "rootPatterns": ["Cargo.toml"]
        },
        "golang": {
            "command": "gopls",
            "args": ["serve", "-debug", "0.0.0.0:8484", "-rpc.trace"],
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"]
        }
    },
    "rust.wait_to_build": 0,
    "tslint.jsEnable": true,
    "pairs.enableCharacters": [
        "(",
        "[",
        "{",
        "'",
        "\"",
        "`"
    ]
}
